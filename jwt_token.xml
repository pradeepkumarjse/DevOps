function generateJwtToken() {
    var currentTime = Math.floor(Date.now() / 1000); // Current time in seconds
    var expirationTime = currentTime + 200; // Expiration time: current time + 200 seconds

    var header = { alg: &apos;RS384&apos;, typ: &apos;JWT&apos; };
    var payload = {
        &quot;iss&quot;: &quot;94e3d9e254e9216&quot;,
        &quot;sub&quot;: &quot;94e3e254e9216&quot;,
        &quot;aud&quot;: &quot;https://fhir.epic.com/interconnect-fhir-oauth/oauth2/token&quot;,
        &quot;jti&quot;: &quot;cd1e70242ac120002&quot; + Math.random(),
        &quot;exp&quot;: expirationTime,
        &quot;iat&quot;: currentTime
    };

    // Convert JavaScript strings to Java strings
    var headerString = new String(JSON.stringify(header));
    var payloadString = new String(JSON.stringify(payload));

    // Base64 encode the header and payload
    var headerBase64 = Base64.encodeBase64URLSafeString(headerString.getBytes(&quot;UTF-8&quot;));
    var payloadBase64 = Base64.encodeBase64URLSafeString(payloadString.getBytes(&quot;UTF-8&quot;));

    // Convert PEM encoded private key to PKCS8 binary format
    var privateKeyPEMFormatted = privateKeyPEM.replace(&quot;-----BEGIN PRIVATE KEY-----&quot;, &quot;&quot;).replace(&quot;-----END PRIVATE KEY-----&quot;, &quot;&quot;).replace(/\s+/g, &quot;&quot;);
    var privateKeyBytes = Base64.decodeBase64(privateKeyPEMFormatted);
    var keySpec = new PKCS8EncodedKeySpec(privateKeyBytes);
    var keyFactory = KeyFactory.getInstance(&quot;RSA&quot;);
    var privateKey = keyFactory.generatePrivate(keySpec);

    // Sign the data
    var signature = Signature.getInstance(&quot;SHA384withRSA&quot;);
    signature.initSign(privateKey);
    var data = new String(headerBase64 + &quot;.&quot; + payloadBase64);
    signature.update(data.getBytes(&quot;UTF-8&quot;));
    var signedBytes = signature.sign();
    var signedBase64 = Base64.encodeBase64URLSafeString(signedBytes);

    // Create the JWT token
    var jwtToken = headerBase64 + &quot;.&quot; + payloadBase64 + &quot;.&quot; + signedBase64;

